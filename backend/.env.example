# Google Gemini API Key (get from https://aistudio.google.com/apikey)
GOOGLE_API_KEY=your_google_api_key_here

# Qdrant Cloud Credentials (get from https://cloud.qdrant.io)
QDRANT_URL=https://your-cluster-id.region.gcp.cloud.qdrant.io:6333
QDRANT_API_KEY=your_qdrant_api_key_here

# Tavily API Key for web search (get from https://tavily.com)
TAVILY_API_KEY=your_tavily_api_key_here

# Hugging Face API Token for offloading CLIP embeddings 
# Get free token at https://huggingface.co/settings/tokens
USE_HF_INFERENCE=true
HF_API_TOKEN=hf_your_token_here
